@import "__constants/variables.styl"
@import "__constants/mixins.styl"

@media only screen and (min-width: 992px) 
	.navbar
		overflow hidden
	.tab
		width 50%
		float left
		& a
			margin-top 0
			text-align center
			width 100%
			display block
			z-index 2
		& >div
			opacity 1
			position: relative
			top 0
			&.place
				top 2.3rem
				padding 0 0 3rem 0


	